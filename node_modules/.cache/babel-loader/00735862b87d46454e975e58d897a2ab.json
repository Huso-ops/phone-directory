{"ast":null,"code":"import 'bulma';\nimport axios from \"axios\";\nimport AddDirectory from './AddDirectory.vue';\nimport ShowDirectory from './ShowDirectory.vue';\nimport UpdateDirectory from './UpdateDirectory.vue';\nexport default {\n  emits: \"look-panel\",\n  components: {\n    AddDirectory,\n    ShowDirectory,\n    UpdateDirectory\n  },\n\n  data() {\n    return {\n      addActive: '',\n      showActive: '',\n      updateActive: '',\n      lists: null,\n      list: '',\n      searchQuery: ''\n    };\n  },\n\n  async mounted() {\n    try {\n      const response = await axios.get('http://localhost:8080/api/phone/get');\n      this.lists = response.data.data;\n    } catch (e) {\n      console.log(e);\n    }\n\n    this.lists.sort(function (a, b) {\n      if (a.name > b.name) {\n        return 1;\n      }\n\n      if (a.name < b.name) {\n        return -1;\n      }\n    });\n  },\n\n  //https://jsonplaceholder.typicode.com/posts\n  //http://localhost:8080/api/phone/get\n  watch: {\n    searchQuery() {\n      if (this.searchQuery.lenght > 0) {\n        let result = this.lists.filter(list => {\n          return list.name.toLowerCase().indexOf(this.searchQuery.toLowerCase()) > -1;\n        });\n        console.log(result);\n      }\n    }\n\n  },\n  methods: {\n    openAddDirectory() {\n      this.addActive = 'is-active';\n    },\n\n    openShow(index) {\n      this.list = this.lists[index];\n      this.showActive = 'is-active';\n    },\n\n    openUpdate(index) {\n      this.list = this.lists[index];\n      this.updateActive = 'is-active';\n    },\n\n    close() {\n      this.addActive = '', this.showActive = '', this.updateActive = '';\n    },\n\n    del(index, id) {\n      const list = this.lists[index];\n      id = list._id;\n\n      if (confirm(\"Are you sure about that ?\")) {\n        axios.delete(`http://localhost:8080/api/phone/delete/` + id).then(response => {\n          this.lists.splice(index, 1);\n          console.log(response);\n        });\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAyDA,OAAO,OAAP;AACA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,YAAP,MAAyB,oBAAzB;AACA,OAAOC,aAAP,MAA0B,qBAA1B;AACA,OAAOC,eAAP,MAA4B,uBAA5B;AAKA,eAAe;EACXC,KAAI,EAAI,YADG;EAEXC,UAAU,EAAC;IACXJ,YADW;IAEXC,aAFW;IAGXC;EAHW,CAFA;;EAOfG,IAAI,GAAE;IACN,OAAO;MACLC,SAAQ,EAAI,EADP;MAELC,UAAS,EAAI,EAFR;MAGLC,YAAW,EAAI,EAHV;MAILC,KAAK,EAAC,IAJD;MAKLC,IAAI,EAAC,EALA;MAMLC,WAAW,EAAC;IANP,CAAP;EASC,CAjBc;;EAmBb,MAAMC,OAAN,GAAgB;IACd,IAAI;MACF,MAAMC,QAAO,GAAI,MAAMd,KAAK,CAACe,GAAN,CAAU,qCAAV,CAAvB;MACA,KAAKL,KAAL,GAAaI,QAAQ,CAACR,IAAT,CAAcA,IAA3B;IAEF,CAJA,CAIE,OAAOU,CAAP,EAAU;MACVC,OAAO,CAACC,GAAR,CAAYF,CAAZ;IAEF;;IACA,KAAKN,KAAL,CAAWS,IAAX,CAAgB,UAASC,CAAT,EAAWC,CAAX,EAAa;MAC7B,IAAGD,CAAC,CAACE,IAAF,GAAOD,CAAC,CAACC,IAAZ,EAAiB;QACf,OAAO,CAAP;MACF;;MAAE,IAAKF,CAAC,CAACE,IAAF,GAAOD,CAAC,CAACC,IAAd,EAAoB;QACpB,OAAO,CAAC,CAAR;MACF;IACC,CAND;EAQD,CApCY;;EAsCb;EACA;EAEAC,KAAK,EAAC;IACJX,WAAW,GAAE;MACd,IAAG,KAAKA,WAAL,CAAiBY,MAAjB,GAA0B,CAA7B,EAA+B;QAC9B,IAAIC,MAAK,GAAI,KAAKf,KAAL,CAAWgB,MAAX,CAAmBf,IAAD,IAAU;UACxC,OAAOA,IAAI,CAACW,IAAL,CAAUK,WAAV,GAAwBC,OAAxB,CAAgC,KAAKhB,WAAL,CAAiBe,WAAjB,EAAhC,IAAgE,CAAC,CAAxE;QACC,CAFW,CAAb;QAGHV,OAAO,CAACC,GAAR,CAAYO,MAAZ;MACG;IAEA;;EATI,CAzCO;EAsDfI,OAAO,EAAC;IACNC,gBAAgB,GAAE;MAChB,KAAKvB,SAAL,GAAiB,WAAjB;IACD,CAHK;;IAKLwB,QAAQ,CAACC,KAAD,EAAO;MAEhB,KAAKrB,IAAL,GAAY,KAAKD,KAAL,CAAWsB,KAAX,CAAZ;MAEC,KAAKxB,UAAL,GAAkB,WAAlB;IACA,CAVK;;IAWNyB,UAAU,CAACD,KAAD,EAAO;MAEhB,KAAKrB,IAAL,GAAY,KAAKD,KAAL,CAAWsB,KAAX,CAAZ;MAEA,KAAKvB,YAAL,GAAoB,WAApB;IACA,CAhBK;;IAmBNyB,KAAK,GAAE;MACL,KAAK3B,SAAL,GAAiB,EAAjB,EACA,KAAKC,UAAL,GAAkB,EADlB,EAEA,KAAKC,YAAL,GAAoB,EAFpB;IAGD,CAvBK;;IAwBN0B,GAAG,CAACH,KAAD,EAAOI,EAAP,EAAU;MACX,MAAMzB,IAAG,GAAI,KAAKD,KAAL,CAAWsB,KAAX,CAAb;MACAI,EAAC,GAAIzB,IAAI,CAAC0B,GAAV;;MACA,IAAGC,OAAM,CAAG,2BAAH,CAAT,EAAyC;QACvCtC,KAAK,CAACuC,MAAN,CAAc,yCAAD,GAA0CH,EAAvD,EACAI,IADA,CACK1B,QAAO,IAAK;UACjB,KAAKJ,KAAL,CAAW+B,MAAX,CAAkBT,KAAlB,EAAwB,CAAxB;UACGf,OAAO,CAACC,GAAR,CAAYJ,QAAZ;QACH,CAJA;MAKF;IAEF;;EAnCM;AAtDO,CAAf","names":["axios","AddDirectory","ShowDirectory","UpdateDirectory","emits","components","data","addActive","showActive","updateActive","lists","list","searchQuery","mounted","response","get","e","console","log","sort","a","b","name","watch","lenght","result","filter","toLowerCase","indexOf","methods","openAddDirectory","openShow","index","openUpdate","close","del","id","_id","confirm","delete","then","splice"],"sourceRoot":"","sources":["C:\\Users\\huso_\\OneDrive\\Masaüstü\\pbb_app\\pb\\src\\components\\LookPanel.vue"],"sourcesContent":["<template>\r\n<div>\r\n<html>\r\n <head>\r\n    <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css\"\r\n          type=\"text/css\">\r\n</head> \r\n<body>\r\n<nav class=\"panel\">\r\n  <p class=\"panel-heading\">\r\n    PhoneBook <br>\r\n        <button class=\"button is-link is-outlined \" @click=\"openAddDirectory\">\r\n        \r\n      Add New\r\n    </button>\r\n  </p>\r\n  <div class=\"panel-block\">\r\n    <p class=\"control has-icons-left\">\r\n      <input class=\"input\" type=\"text\" placeholder=\"Search\" v-model=\"searchQuery\">\r\n      <span class=\"icon is-left\">\r\n        <i class=\"fas fa fa-search\" aria-hidden=\"true\"></i>\r\n      </span>\r\n    </p>\r\n  </div>\r\n\r\n  <a class=\"panel-block\" v-for=\"list,index in lists\" v-bind:key=\"list.id\">    \r\n<span class=\" column is-9\">\r\n  {{list.name}}\r\n</span>\r\n\r\n<span class=\"panel-icon  is-1\">\r\n<i class=\"has-text-danger fa-solid fa fa-trash\" aria-hidden=\"true\" @click=\"del(index,list.id)\"></i>\r\n  </span>\r\n\r\n<span class=\"panel-icon column is-2\">\r\n<i class=\"has-text-info fa-solid fa fa-edit\" aria-hidden=\"true\"  @click=\"openUpdate(index)\"></i>\r\n    </span>       \r\n\r\n<span class=\"panel-icon  is-3\">\r\n<i class=\"has-text-primary fa-solid fa fa-eye\" aria-hidden=\"true\" @click=\"openShow(index)\"></i>\r\n    </span>\r\n  </a>\r\n  \r\n\r\n</nav>\r\n\r\n<AddDirectory :openmodal='addActive' @closeRequest='close' :lists=\"this.lists\"></AddDirectory>\r\n\r\n<ShowDirectory :openmodal='showActive' @closeRequest='close' :list=\"this.list\"></ShowDirectory>\r\n\r\n<UpdateDirectory :openmodal='updateActive' @closeRequest='close' :list=\"this.list\"></UpdateDirectory>\r\n</body>\r\n</html>\r\n</div>\r\n\r\n</template>\r\n<script >\r\nimport 'bulma'\r\nimport axios from \"axios\";\r\nimport AddDirectory from './AddDirectory.vue';\r\nimport ShowDirectory from './ShowDirectory.vue'\r\nimport UpdateDirectory from './UpdateDirectory.vue';\r\n\r\n\r\n\r\n\r\nexport default {\r\n    emits : \"look-panel\",\r\n    components:{\r\n    AddDirectory,\r\n    ShowDirectory,\r\n    UpdateDirectory\r\n},\r\ndata(){\r\nreturn {\r\n  addActive : '',\r\n  showActive : '',\r\n  updateActive : '',\r\n  lists:null,\r\n  list:'',\r\n  searchQuery:''\r\n\r\n}\r\n},\r\n\r\n  async mounted() {\r\n    try {\r\n      const response = await axios.get('http://localhost:8080/api/phone/get');\r\n      this.lists = response.data.data\r\n      \r\n    } catch (e) {\r\n      console.log(e)\r\n    \r\n    }\r\n    this.lists.sort(function(a,b){\r\n    if(a.name>b.name){\r\n      return 1;\r\n    } if ((a.name<b.name)){\r\n      return -1;\r\n    }\r\n    })\r\n    \r\n  },\r\n\r\n  //https://jsonplaceholder.typicode.com/posts\r\n  //http://localhost:8080/api/phone/get\r\n\r\n  watch:{\r\n    searchQuery(){\r\n   if(this.searchQuery.lenght > 0){\r\n    let result = this.lists.filter((list) => {\r\n     return list.name.toLowerCase().indexOf(this.searchQuery.toLowerCase())>-1\r\n     });\r\n console.log(result)\r\n    }\r\n   \r\n    }\r\n  \r\n  },\r\n\r\nmethods:{\r\n  openAddDirectory(){\r\n    this.addActive = 'is-active';\r\n  },\r\n  \r\n   openShow(index){\r\n\r\n  this.list = this.lists[index];\r\n\r\n   this.showActive = 'is-active';\r\n  },\r\n  openUpdate(index){\r\n\r\n   this.list = this.lists[index];\r\n\r\n   this.updateActive = 'is-active';\r\n  },\r\n\r\n\r\n  close(){\r\n    this.addActive = '',\r\n    this.showActive = '',\r\n    this.updateActive = ''\r\n  },\r\n  del(index,id){\r\n    const list = this.lists[index];\r\n    id = list._id;\r\n    if(confirm (\"Are you sure about that ?\")){\r\n      axios.delete(`http://localhost:8080/api/phone/delete/`+id)\r\n     .then(response => {\r\n      this.lists.splice(index,1);\r\n         console.log(response);\r\n     });\r\n    }\r\n\r\n  }\r\n\r\n},\r\n\r\n};\r\n</script>\r\n<style>\r\n</style>\r\n\r\n\r\n"]},"metadata":{},"sourceType":"module"}