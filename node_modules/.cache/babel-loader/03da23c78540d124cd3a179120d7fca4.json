{"ast":null,"code":"import axios from 'axios';\nexport default {\n  emits: \"update-directory\",\n  props: ['openmodal', 'list'],\n\n  data() {\n    return {\n      formEdit: {\n        name: \"\",\n        phoneNumber: \"\",\n        email: \"\"\n      }\n    };\n  },\n\n  errors: {},\n  methods: {\n    close() {\n      this.$emit('closeRequest');\n    },\n\n    async update() {\n      let _id = this.$props.list._id;\n      let name = this.$props.list.name;\n      let phoneNumber = this.$props.list.phoneNumber;\n      let email = this.$props.list.email;\n\n      try {\n        const response = await axios.put('http://localhost:8080/api/phone/update/' + _id, name, phoneNumber, email);\n        response.data.data.name = name, phoneNumber = response.data.data.phoneNumber, email = response.data.data.email;\n      } catch (err) {\n        console.log(err);\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA2CA,OAAOA,KAAP,MAAkB,OAAlB;AAIA,eAAe;EACVC,KAAI,EAAI,kBADE;EAEbC,KAAK,EAAE,CAAC,WAAD,EAAa,MAAb,CAFM;;EAGfC,IAAI,GAAE;IACJ,OAAO;MACTC,QAAO,EAAI;QACPC,IAAI,EAAC,EADE;QAELC,WAAW,EAAC,EAFP;QAGLC,KAAK,EAAC;MAHD;IADF,CAAP;EAcD,CAlBc;;EAoBbC,MAAM,EAAC,EApBM;EAsBbC,OAAO,EAAC;IACNC,KAAK,GAAE;MACL,KAAKC,KAAL,CAAW,cAAX;IACD,CAHK;;IAIP,MAAMC,MAAN,GAAc;MACb,IAAKC,GAAE,GAAI,KAAKC,MAAL,CAAYC,IAAZ,CAAiBF,GAA5B;MACA,IAAIR,IAAG,GAAI,KAAKS,MAAL,CAAYC,IAAZ,CAAiBV,IAA5B;MACA,IAAIC,WAAU,GAAI,KAAKQ,MAAL,CAAYC,IAAZ,CAAiBT,WAAnC;MACA,IAAIC,KAAI,GAAI,KAAKO,MAAL,CAAYC,IAAZ,CAAiBR,KAA7B;;MAEA,IAAG;QACH,MAAMS,QAAO,GAAI,MAAOhB,KAAK,CAACiB,GAAN,CAAU,4CAA2CJ,GAArD,EAAyDR,IAAzD,EAA8DC,WAA9D,EAA0EC,KAA1E,CAAxB;QAGES,QAAQ,CAACb,IAAT,CAAcA,IAAd,CAAmBE,IAAnB,GAA0BA,IAA1B,EACFC,WAAU,GAAIU,QAAQ,CAACb,IAAT,CAAcA,IAAd,CAAmBG,WAD/B,EAEFC,KAAI,GAAMS,QAAQ,CAACb,IAAT,CAAcA,IAAd,CAAmBI,KAF3B;MAID,CARD,CAQC,OAAMW,GAAN,EAAU;QACTC,OAAO,CAACC,GAAR,CAAYF,GAAZ;MAEF;IAEA;;EAvBM;AAtBK,CAAf","names":["axios","emits","props","data","formEdit","name","phoneNumber","email","errors","methods","close","$emit","update","_id","$props","list","response","put","err","console","log"],"sourceRoot":"","sources":["C:\\Users\\huso_\\OneDrive\\Masaüstü\\pb-app\\pb\\src\\components\\UpdateDirectory.vue"],"sourcesContent":["<template>\r\n<div class=\"modal \" :class='openmodal'>\r\n  <div class=\"modal-background\"></div>\r\n  <div class=\"modal-card\">\r\n    <header class=\"modal-card-head\">\r\n      <p class=\"modal-card-title\">Update {{list.name}}'s Details'</p>\r\n     \r\n      <button class=\"delete\" aria-label=\"close\" @click=\"close\"></button>\r\n    </header>\r\n    <section class=\"modal-card-body\">\r\n\r\n     <div class=\"field\">\r\n  <label class=\"label\">Name</label>\r\n  <div class=\"control\">\r\n    <input ref=\"list\" class=\"input\" type=\"text\" placeholder=\"Name\" v-model=\"formEdit.name\" required=\"true\">\r\n    \r\n\r\n  </div>\r\n</div>\r\n     <div class=\"field\">\r\n  <label class=\"label\">phone</label>\r\n  <div class=\"control\">\r\n    <input class=\"input\" type=\"number\" placeholder=\"Phone\" v-model=\"this.$props.list.phoneNumber\"  required=\"true\">\r\n    \r\n  </div>\r\n</div>\r\n     <div class=\"field\">\r\n  <label class=\"label\">email</label>\r\n  <div class=\"control\">\r\n    <input class=\"input\" type=\"text\" placeholder=\"Email\" v-model=\"this.$props.list.email\" required=\"true\">\r\n    \r\n  </div>\r\n</div>\r\n    </section>\r\n    <footer class=\"modal-card-foot\">\r\n      <button class=\"button is-success\" @click=\"update\">Update</button>\r\n      <button class=\"button\" @click=\"close\">Cancel</button>\r\n    </footer>\r\n  </div>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\n\r\n\r\nexport default {\r\n     emits : \"update-directory\",\r\n  props: ['openmodal','list'],\r\ndata(){\r\n  return {\r\nformEdit : {\r\n    name:\"\",\r\n      phoneNumber:\"\",\r\n      email:\"\"\r\n  \r\n}\r\n\r\n   \r\n     \r\n  \r\n   \r\n\r\n  }\r\n},\r\n\r\n  errors:{},\r\n\r\n  methods:{\r\n    close(){\r\n      this.$emit('closeRequest')\r\n    },\r\n   async update(){\r\n    let  _id = this.$props.list._id\r\n    let name = this.$props.list.name\r\n    let phoneNumber = this.$props.list.phoneNumber\r\n    let email = this.$props.list.email\r\n      \r\n    try{\r\n    const response = await  axios.put('http://localhost:8080/api/phone/update/' +_id,name,phoneNumber,email \r\n    );\r\n      \r\n      response.data.data.name = name ,\r\n    phoneNumber = response.data.data.phoneNumber ,\r\n    email =   response.data.data.email \r\n\r\n    }catch(err){\r\n      console.log(err);\r\n      \r\n    }\r\n     \r\n    }\r\n\r\n  }\r\n\r\n};\r\n\r\n</script>"]},"metadata":{},"sourceType":"module"}